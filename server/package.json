{
  "name": "aspiritywebtemplate-server",
  "version": "0.1.0",
  "description": "Aspirity Web Template Server",
  "license": "ISC",
  "author": "Aspirity LTD",
  "scripts": {
    "server": "nodemon --exec ts-node --project ./src/tsconfig.json ./src/server.ts",
    "dependencies": "nodemon --config nodemon-dependencies.json --exec yarn install",
    "start": "concurrently 'yarn run server' 'yarn run dependencies'"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "convict": "^4.2.0",
    "express": "^4.16.3",
    "express-winston": "^2.5.1",
    "mongoose": "^5.1.0",
    "typegoose": "^5.2.1",
    "winston": "^2.4.2",
    "winston-daily-rotate-file": "^3.1.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.11.1",
    "@types/express-winston": "^2.4.3",
    "@types/mongoose": "^5.0.13",
    "@types/node": "^10.0.6",
    "@types/winston": "^2.3.9",
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.4",
    "ts-node": "^6.0.3",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  }
}
